    "# decorators are hard\n",
    "\n",
    "def dec(x):\n",
    "    def _fn(obj):\n",
    "        print(x, obj)\n",
    "        return obj\n",
    "    return _fn\n",
    "\n",
    "@dec(\"asdf\")\n",
    "class A:\n",
    "    pass\n",
